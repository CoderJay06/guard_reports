<h4>All Reports</h4>
  <ul>
    <% @reports.each do |report| %>
    <br>
    <li><a href="/reports/<%= report.id %>">View Report</a></li>
    <li>Report Type: <%= report.report_type %></li>
    <li>Report Date: <%= report.date %></li>
    <li>Report Time: <%= report.time %></li>
    <li>Report Location: <%= report.location %></li>
    <li>Report Description: <%= report.description %></li>
    <br>
    <% end %>
  </ul>